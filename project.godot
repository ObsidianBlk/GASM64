; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "ASMEdit",
"language": "GDScript",
"path": "res://UI/ASMEdit/ASMEdit.gd"
}, {
"base": "Node",
"class": "Bus",
"language": "GDScript",
"path": "res://Scripts/Com/Bus.gd"
}, {
"base": "Node",
"class": "CPU6502",
"language": "GDScript",
"path": "res://Scripts/Com/CPU6502.gd"
}, {
"base": "Node",
"class": "Clock",
"language": "GDScript",
"path": "res://Scripts/Com/Clock.gd"
}, {
"base": "Reference",
"class": "Lexer",
"language": "GDScript",
"path": "res://Scripts/Assembler/Lexer/Lexer.gd"
}, {
"base": "Container",
"class": "MCLine",
"language": "GDScript",
"path": "res://UI/ASMEdit/DataView/MCLine.gd"
}, {
"base": "Node",
"class": "MemDevice",
"language": "GDScript",
"path": "res://Scripts/Com/MemDevice.gd"
}, {
"base": "MemDevice",
"class": "Memory",
"language": "GDScript",
"path": "res://Scripts/Com/Memory.gd"
} ]
_global_script_class_icons={
"ASMEdit": "",
"Bus": "",
"CPU6502": "",
"Clock": "",
"Lexer": "",
"MCLine": "",
"MemDevice": "",
"Memory": ""
}

[application]

config/name="GASM64"
config/description="Generally Awesome Simulated Machine 64k Edition!"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

GASM="*res://Scripts/Auto/GASM.gd"
Utils="*res://Scripts/Auto/Utils.gd"

[display]

window/size/width=1920
window/size/height=1080

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
